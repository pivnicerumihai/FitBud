<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitbud">

    <application
        android:name=".Model.Global"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.NoActionBar">
        <activity
            android:name=".ResetPasswordActivity"
            android:exported="true" />
        <activity
            android:name=".WorkoutExercisesAdapter"
            android:exported="true" />
        <activity
            android:name=".GoalsActivity"
            android:exported="true" />
        <activity
            android:name=".RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".SingleRecipeActivity"
            android:exported="true" />
        <activity
            android:name=".RecipeListAdapter"
            android:exported="true" />
        <activity
            android:name=".PerformWorkoutActivity"
            android:exported="true" />
        <activity
            android:name=".RecipesList"
            android:exported="true" />
        <activity
            android:name=".RecipeCategory"
            android:exported="true" />
        <activity
            android:name=".StretchesList"
            android:exported="true" />
        <activity
            android:name=".AddExerciseActivity"
            android:exported="true" />
        <activity
            android:name=".ExerciseDetailsActivity"
            android:exported="true" />
        <activity
            android:name=".WorkoutActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".ExercisesList"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ExercisesActivity" />
        </activity>
        <activity
            android:name=".ExercisesActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>